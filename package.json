{
    "name": "jihyung",
    "productName": "JIHYUNG",
    "description": "지형이의 개인 생산성 관리 시스템",
    "author": "Your Name <your.email@example.com>",
    "homepage": "https://github.com/yourusername/spark-ai",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "main": "electron.cjs",
    "scripts": {
        "dev": "concurrently -k \"npm:dev:fe\" \"npm:dev:be\"",
        "dev:fe": "vite --host --port 5178",
        "dev:be": "bash -c 'source backend_env/bin/activate && uvicorn backend.main_enhanced:app --host 0.0.0.0 --port 8006 --reload'",
        "build": "vite build",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit",
        "format": "prettier -w .",
        "optimize": "vite optimize",
        "preview": "vite preview",
        "test:e2e": "playwright test",
        "electron": "concurrently -k \"npm:dev:fe\" \"npm:dev:be\" \"wait-on http://localhost:5178 && electron .\"",
        "electron:dev": "electron .",
        "electron:pack": "npm run build && npx electron-builder",
        "electron:dist": "npm run build && npx electron-builder --publish=never",
        "app:dev": "npm run electron",
        "app:build": "npm run electron:pack"
    },
    "dependencies": {
        "@fullcalendar/core": "6.1.15",
        "@fullcalendar/daygrid": "6.1.15",
        "@fullcalendar/interaction": "6.1.15",
        "@fullcalendar/list": "6.1.15",
        "@fullcalendar/react": "6.1.15",
        "@fullcalendar/timegrid": "6.1.15",
        "@heroicons/react": "^2.2.0",
        "@hookform/resolvers": "^4.1.3",
        "@octokit/core": "^6.1.4",
        "@phosphor-icons/react": "^2.1.7",
        "@radix-ui/colors": "^3.0.0",
        "@radix-ui/react-accordion": "^1.2.3",
        "@radix-ui/react-alert-dialog": "^1.1.6",
        "@radix-ui/react-aspect-ratio": "^1.1.2",
        "@radix-ui/react-avatar": "^1.1.3",
        "@radix-ui/react-checkbox": "^1.1.4",
        "@radix-ui/react-collapsible": "^1.1.3",
        "@radix-ui/react-context-menu": "^2.2.6",
        "@radix-ui/react-dialog": "^1.1.6",
        "@radix-ui/react-dropdown-menu": "^2.1.6",
        "@radix-ui/react-hover-card": "^1.1.6",
        "@radix-ui/react-label": "^2.1.2",
        "@radix-ui/react-menubar": "^1.1.6",
        "@radix-ui/react-navigation-menu": "^1.2.5",
        "@radix-ui/react-popover": "^1.1.6",
        "@radix-ui/react-progress": "^1.1.2",
        "@radix-ui/react-radio-group": "^1.2.3",
        "@radix-ui/react-scroll-area": "^1.2.9",
        "@radix-ui/react-select": "^2.1.6",
        "@radix-ui/react-separator": "^1.1.2",
        "@radix-ui/react-slider": "^1.2.3",
        "@radix-ui/react-slot": "^1.1.2",
        "@radix-ui/react-switch": "^1.1.3",
        "@radix-ui/react-tabs": "^1.1.3",
        "@radix-ui/react-toggle": "^1.1.2",
        "@radix-ui/react-toggle-group": "^1.1.2",
        "@radix-ui/react-tooltip": "^1.1.8",
        "@react-three/drei": "^10.7.5",
        "@react-three/fiber": "^9.3.0",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/vite": "^4.1.11",
        "@tanstack/react-query": "^5.87.1",
        "@types/d3": "^7.4.3",
        "@types/three": "^0.180.0",
        "axios": "^1.11.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "crypto-js": "^4.2.0",
        "d3": "^7.9.0",
        "date-fns": "^3.6.0",
        "embla-carousel-react": "^8.5.2",
        "framer-motion": "^12.23.12",
        "idb": "^8.0.3",
        "input-otp": "^1.4.2",
        "lucide-react": "^0.484.0",
        "marked": "^15.0.7",
        "next-themes": "^0.4.6",
        "octokit": "^4.1.2",
        "preact": "^10.27.2",
        "react": "^19.0.0",
        "react-day-picker": "^9.6.7",
        "react-dom": "^19.0.0",
        "react-error-boundary": "^6.0.0",
        "react-hook-form": "^7.54.2",
        "react-resizable-panels": "^2.1.7",
        "recharts": "^2.15.4",
        "rrule": "^2.8.1",
        "sonner": "^2.0.1",
        "tailwind-merge": "^3.0.2",
        "three": "^0.175.0",
        "tw-animate-css": "^1.2.4",
        "uuid": "^11.1.0",
        "vaul": "^1.1.2",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@playwright/test": "^1.54.2",
        "@tailwindcss/postcss": "^4.1.8",
        "@types/crypto-js": "^4.2.2",
        "@types/node": "^22.17.1",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitejs/plugin-react-swc": "^3.10.1",
        "concurrently": "^9.2.0",
        "eslint": "^9.28.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "prettier": "^3.6.2",
        "tailwindcss": "^4.1.11",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.38.0",
        "vite": "^6.3.5",
        "wait-on": "^8.0.1"
    },
    "optionalDependencies": {
        "electron": "^32.2.1",
        "electron-builder": "^24.13.3"
    },
    "build": {
        "appId": "com.yourcompany.spark-ai",
        "productName": "Spark AI",
        "copyright": "Copyright © 2025 Your Company",
        "directories": {
            "output": "release"
        },
        "files": [
            "dist/**/*",
            "backend/**/*",
            "electron.cjs",
            "preload.cjs",
            "package.json",
            "!backend/**/*.pyc",
            "!backend/**/__pycache__",
            "!backend/**/.git*",
            "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
            "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
            "!**/node_modules/*.d.ts",
            "!**/node_modules/.bin",
            "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
            "!.editorconfig",
            "!**/._*",
            "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
            "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
            "!**/{appveyor.yml,.travis.yml,circle.yml}",
            "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
        ],
        "extraResources": [
            {
                "from": "backend/backend_env",
                "to": "backend_env",
                "filter": ["**/*", "!**/*.pyc", "!**/__pycache__"]
            }
        ],
        "mac": {
            "category": "public.app-category.productivity",
            "icon": "assets/icon.icns",
            "target": [
                {
                    "target": "dmg",
                    "arch": ["x64", "arm64"]
                },
                {
                    "target": "mas",
                    "arch": ["x64", "arm64"]
                }
            ],
            "hardenedRuntime": true,
            "entitlements": "assets/entitlements.mac.plist",
            "entitlementsInherit": "assets/entitlements.mac.plist",
            "notarize": false
        },
        "mas": {
            "entitlements": "assets/entitlements.mas.plist",
            "entitlementsInherit": "assets/entitlements.mas.inherit.plist",
            "hardenedRuntime": false
        },
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": ["x64", "ia32"]
                },
                {
                    "target": "appx",
                    "arch": ["x64"]
                }
            ],
            "icon": "assets/icon.ico",
            "publisherName": "Your Company Name"
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true,
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true
        },
        "appx": {
            "applicationId": "SparkAI",
            "backgroundColor": "#463714",
            "showNameOnTiles": true,
            "identityName": "YourCompany.SparkAI",
            "publisher": "CN=Your Company Name",
            "publisherDisplayName": "Your Company Name"
        },
        "linux": {
            "target": [
                {
                    "target": "AppImage",
                    "arch": ["x64"]
                },
                {
                    "target": "deb",
                    "arch": ["x64"]
                },
                {
                    "target": "rpm",
                    "arch": ["x64"]
                }
            ],
            "icon": "assets/icon.png",
            "category": "Office"
        },
        "publish": null
    }
}

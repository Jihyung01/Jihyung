# === Frontend API Configuration ===
VITE_API_BASE=http://localhost:8004

# === Development ===
# VITE_DEBUG=true

# === Optional Auth ===
# When AUTH_ENABLED=true on backend, app will require login
# Token is stored in localStorage automatically after login

# === OAuth Configuration (Backend) ===
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:5177/auth/google/callback

GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here
GITHUB_REDIRECT_URI=http://localhost:5177/auth/github/callback

KAKAO_CLIENT_ID=your_kakao_client_id_here
KAKAO_CLIENT_SECRET=your_kakao_client_secret_here
KAKAO_REDIRECT_URI=http://localhost:5177/auth/kakao/callback

# === Database ===
DATABASE_URL=postgresql://user:password@localhost:5432/spark_db

# === OpenAI ===
OPENAI_API_KEY=your_openai_api_key_here

# === JWT ===
JWT_SECRET_KEY=your_super_secret_jwt_key_here
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# === Redis (Optional) ===
REDIS_URL=redis://localhost:6379

# === AWS (Optional for file uploads) ===
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1
AWS_S3_BUCKET=your_s3_bucket_here